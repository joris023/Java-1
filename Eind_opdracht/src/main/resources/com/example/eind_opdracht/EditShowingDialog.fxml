<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<VBox spacing="10" xmlns="http://javafx.com/javafx/17.0.12" xmlns:fx="http://javafx.com/fxml/1">
<children>
    <VBox spacing="10">
   <Label text="Edit showing">
      <font>
         <Font size="20.0" />
      </font>
      <padding>
         <Insets left="20.0" />
      </padding>
   </Label>
   <VBox id="WhiteBack" prefHeight="200.0" prefWidth="100.0" spacing="10.0">
      <children>
         <TextField fx:id="titleShowingTextField" prefHeight="26.0" prefWidth="200.0" promptText="Title">
            <VBox.margin>
               <Insets right="150.0" />
            </VBox.margin></TextField>
         <Label text="Start date/time:" />
         <HBox prefHeight="6.0" prefWidth="559.0">
            <children>
               <DatePicker fx:id="startDatePicker" promptText="Date (MM/DD/YYYY)" />
               <TextField fx:id="startTimePicker" promptText="Time (HH:MM)" />
            </children>
         </HBox>
         <Label text="End date/time:" />
         <HBox prefHeight="21.0" prefWidth="559.0">
            <children>
               <DatePicker fx:id="endDatePicker" promptText="Date (MM/DD/YYYY)" />
               <TextField fx:id="endTimePicker" promptText="Time (HH:MM)" />
               <CheckBox mnemonicParsing="false" text="16+ movie" fx:id="sixteenPlusBtn"/>
            </children>
         </HBox>
      </children>
      <padding>
         <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
      </padding>
      <VBox.margin>
         <Insets left="10.0" right="10.0" />
      </VBox.margin>
   </VBox>
   <HBox prefHeight="28.0" prefWidth="580.0" spacing="10.0">
      <children>
         <Button id="BlueBack" mnemonicParsing="false" onAction="#editShowingPress" text="Edit showing" />
         <Button mnemonicParsing="false" onAction="#cancelPress" text="Cancel" />
      </children>
      <VBox.margin>
         <Insets left="20.0" />
      </VBox.margin>
   </HBox>
    </VBox>
</children>
   <padding>
      <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
   </padding>
</VBox>
