<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<VBox prefHeight="278.0" prefWidth="350" spacing="10" xmlns="http://javafx.com/javafx/17.0.12" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <Label text="Age check required">
         <font>
            <Font size="20.0" />
         </font>
      </Label>
      <Label fx:id="movieNameLabel" text="Movie: " />
      <Label fx:id="dateAndTimeLabel" text="Date and Time:" />
      <Label fx:id="numberTicketsLabel" text="Number of tickets:" />
      <Label fx:id="customerNameLabel" text="Customer name" />
      <CheckBox fx:id="sixteenCheckbox" mnemonicParsing="false" prefHeight="109.0" prefWidth="310.0" text="I have checked the ID card &#10; and confirmed that the customer is at least 16 years old" />
      <HBox prefHeight="51.0" prefWidth="350.0" spacing="10.0">
         <children>
            <Button id="BlueBack" fx:id="confirmBtn" mnemonicParsing="false" onAction="#confirmPress" text="Confirm purchase" disable="true"/>
            <Button mnemonicParsing="false" onAction="#cancelPress" text="Cancel" />
         </children></HBox>
   </children>
   <padding>
      <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
   </padding>
</VBox>
